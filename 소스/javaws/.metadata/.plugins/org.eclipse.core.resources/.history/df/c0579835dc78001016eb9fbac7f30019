package hello;

public class 일십까지의합 {

	public static void main(String[] args) {
		// TODO Auto-generated method stub
		
		// 변수 선언
		  int Sum=0; int k=0;
		 //
		 final int MAX=10;
		
		// 반복문
		
		for ( int i=0; i<=MAX; i++) {
			
			k=i+1;
			Sum=Sum+k;
		}
		
		
		System.out.println(Sum);
		

	}

}
